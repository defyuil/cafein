<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- MaintMapper.xml -->
<mapper namespace="com.cafein.mapper.MainMapper">

	<select id="loginMember" resultType="com.cafein.domain.MemberVO">
		select * from member
		where membercode=#{membercode} and memberpw=#{memberpw}
	</select>

</mapper>